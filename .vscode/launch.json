{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug os with os_syscall",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/os",
      "args": ["kill_process"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "build os",
      "miDebuggerPath": "/usr/bin/gdb"
    }
  ]
}
